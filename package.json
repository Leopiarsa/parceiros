{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint \"src\"",
    "dev": "tsx --watch src/app.ts",
    "generate-bundles": "rm -rf ./dist && tsc && node dist/src/admin/bundle-admin.js",
    "prod": "export NODE_ENV=production && export ADMIN_JS_SKIP_BUNDLE=\"true\" && tsx src/app.ts",
    "typeorm": "tsx node_modules/.bin/typeorm",
    "migration:generate": "tsx node_modules/.bin/typeorm migration:generate -d src/db/data-source.ts",
    "migration:run": "tsx node_modules/.bin/typeorm migration:run -d src/db/data-source.ts",
    "migration:rollback": "tsx node_modules/.bin/typeorm migration:revert -d src/db/data-source.ts"
  },
  "dependencies": {
    "@adminjs/bundler": "^3.0.0",
    "@adminjs/express": "^6.1.0",
    "@adminjs/typeorm": "^5.0.1",
    "@adminjs/upload": "^4.0.2",
    "@aws-sdk/s3-request-presigner": "^3.779.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^6.4.10",
    "@mui/material": "^6.4.7",
    "@mui/x-data-grid": "^7.28.1",
    "@mui/x-date-pickers": "^7.28.0",
    "@mui/x-date-pickers-pro": "^7.28.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@sendgrid/mail": "^8.1.4",
    "adminjs": "^7.7.2",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "dayjs": "^1.11.13",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "http-browserify": "^1.7.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pg": "latest",
    "react-input-mask": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zlib-browserify": "^0.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.3.20",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.3",
    "prettier": "^3.1.0",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.0.9",
    "tslib": "^2.5.0",
    "tsx": "^4.19.3",
    "typeorm": "^0.3.21",
    "typescript": "^5.0.4"
  },
  "name": "express_partnerleads"
}
